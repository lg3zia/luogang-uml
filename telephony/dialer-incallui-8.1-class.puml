enum InCallState{
NO_CALLS
    INCOMING
    INCALL
    WAITING_FOR_ACCOUNT
    PENDING_OUTGOING
    OUTGOING
}
class ContactInfoCache {
    findInfo()
    onImageLoaded()
}
class InCallPresenter{
    setActivity()
    unsetActivity()
    getInCallState()
    getCallList()
    ...()
}
class StatusBarNotifier{
    updateNotification()
}
/'interface InCallStateListener{
    onStateChange()
}
interface IncomingCallListener{
    onIncomingCall()
}
interface CanAddCallListener{
    onCanAddCallChanged()
}
interface InCallDetailsListener{
    onDetailsChanged()
}
interface InCallOrientationListener{
