class CallCardPresenter{
    主presenter：
    1.监听incallstate
    2.子fragment
}
class LocationFragment{
    上部分：
}
class LockableViewPager{
    中部分：
    1.按钮组合
    2.MultimediaFragment
}
class CallButtonPresenter{
    中部分：
    1.按钮点击事件
    2.call状态变化更新按钮
}
class ContactGridManager{
    上部分：
    1.头像
    2.通话连接符号、连接状态
    3.姓名|号码
    4.各类icon|电话号码|时长
}
/'=======================InCallFragment end'/

